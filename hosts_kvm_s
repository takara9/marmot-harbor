###
### ノードのリストとアクセス方法
###

harbor    ansible_connection=local


###
### プレイブックに与える変数
###
[all:vars]

# for Vagrant
#admin_user = vagrant
#disk1 = /dev/sdc
#disk2 = /dev/sdd
#public_ip_addr = {{ ansible_facts.enp0s9.ipv4.address }}

# for KVM
#admin_user = ubuntu
#disk1 = /dev/vdb
#disk2 = /dev/vdc
#public_ip_addr = "192.168.1.221"